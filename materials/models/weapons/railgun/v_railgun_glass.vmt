VertexLitGeneric{
$baseTexture models/weapons/railgun/v_railgun_glass
$bumpmap models/weapons/railgun/v_railgun_glass_normal
$translucent 1
$additive 1
$envmap env_cubemap
$envmaptint "[0.2  0.2 0.5]"

Proxies{
TextureScroll{
texturescrollvar "$basetexturetransform"
texturescrollrate .2
texturescrollangle 180
}
TextureScroll{
texturescrollvar "$bumptransform"
texturescrollrate .1
texturescrollangle 0
}
}
}