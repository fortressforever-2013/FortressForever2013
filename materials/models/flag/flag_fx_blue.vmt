VertexLitGeneric{
$baseTexture models/flag/flag_blue_glow
$bumpmap effects/chaos_glow_normal
$additive 1
$translucent 1

//Environment Mapping
$envmap env_cubemap
$envmaptint "[0 0 1]"

//Local Variables
$defaultTint "[0.1 0.1 0.9]"

Proxies{
TextureScroll{
texturescrollvar "$basetexturetransform"
texturescrollrate .2
texturescrollangle 180
}
TextureScroll{
texturescrollvar "$bumptransform"
texturescrollrate .1
texturescrollangle 0
}
Equals{
srcVar1 "$defaultTint"
resultVar "$color2"
}
}
}