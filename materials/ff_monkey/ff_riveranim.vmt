UnlitGeneric{
$basetexture ff_monkey/ff_riveranim
$translucent 1
$nocull 1
$additive 1
Proxies{
TextureScroll{
texturescrollvar "$baseTextureTransform"
texturescrollrate .15
texturescrollangle 270
}
AnimatedTexture{
animatedtexturevar $basetexture
animatedtextureframenumvar $frame
animatedtextureframerate 12
}
}
}